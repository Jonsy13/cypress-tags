{
  "name": "cypress-tags",
  "version": "0.0.12",
  "description": "Use custom tags to slice up Cypress test runs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --spec test/tagify.js",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "author": "Anna Thomas",
  "license": "ISC",
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "prepare": "npm run build",
  "postversion": "git push && git push --tags",
  "repository": {
    "type": "git",
    "url": "https://github.com/annaet/cypress-tags.git"
  },
  "dependencies": {
    "@cypress/browserify-preprocessor": "^3.0.1",
    "cypress": "^5.5.0",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "@types/through": "0.0.30",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "cypress",
    "tags",
    "tagging",
    "markers",
    "test",
    "testing"
  ]
}
